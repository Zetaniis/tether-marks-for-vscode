{
  "name": "tether-marks-for-vscode",
  "displayName": "tether-marks-for-vscode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tether-marks-for-vscode.set-mark",
        "title": "Tether marks: Set mark"
      },
      {
        "command": "tether-marks-for-vscode.go-to-mark",
        "title": "Tether marks: Go to mark"
      },
      {
        "command": "tether-marks-for-vscode.add-file-to-harpoon",
        "title": "Tether marks: Add file to harpoon"
      },
      {
        "command": "tether-marks-for-vscode.go-to-harpoon-mark",
        "title": "Tether marks: Go to harpoon mark"
      },
      {
        "command": "tether-marks-for-vscode.delete-mark",
        "title": "Tether marks: Delete mark"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
